    <keyMapSet id="JIS">
        <!-- I don't use JIS and I don't ever plan on using it,
    	so I'm going to leave this entire section alone.
    	It's small enough that I think JIS people can figure it
    	out pretty well based on their existing knowledge,
    	whereas I am clueless on how JIS even works. -->
        <keyMap index="0" baseMapSet="ANSIISO" baseIndex="0">
            <key code="24" output="^"/>
            <key code="30" output="["/>
            <key code="33" output="@"/>
            <key code="39" output=":"/>
            <key code="42" output="]"/>
            <key code="93" output="¥"/>
            <key code="94" output="_"/>
            <key code="95" output=","/>
            <key code="102" action="spacebar"/>
            <key code="104" action="spacebar"/>
        </keyMap>
