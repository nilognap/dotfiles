    <actions>
        <!-- The actions section defines how
        dead keys/combining diacritics work in
        the layout. There is probably no reason to
        change this section at all. -->
        <!-- The first five actions correspond to the
        dead keys themselves when they are pressed. They
        simply pull the system into the next state. -->
        <action id="A">
            <when state="none" output="A"/>
            <when state="1" output="Á"/>
            <when state="2" output="À"/>
            <when state="3" output="Â"/>
            <when state="4" output="Ä"/>
            <when state="5" output="Ã"/>
        </action>
        <action id="E">
            <when state="none" output="E"/>
            <when state="1" output="É"/>
            <when state="2" output="È"/>
            <when state="3" output="Ê"/>
            <when state="4" output="Ë"/>
        </action>
        <action id="I">
            <when state="none" output="I"/>
            <when state="1" output="Í"/>
            <when state="2" output="Ì"/>
            <when state="3" output="Î"/>
            <when state="4" output="Ï"/>
        </action>
        <action id="N">
            <when state="none" output="N"/>
            <when state="5" output="Ñ"/>
        </action>
        <action id="O">
            <when state="none" output="O"/>
            <when state="1" output="Ó"/>
            <when state="2" output="Ò"/>
            <when state="3" output="Ô"/>
            <when state="4" output="Ö"/>
            <when state="5" output="Õ"/>
        </action>
        <action id="U">
            <when state="none" output="U"/>
            <when state="1" output="Ú"/>
            <when state="2" output="Ù"/>
            <when state="3" output="Û"/>
            <when state="4" output="Ü"/>
        </action>
        <action id="Y">
            <when state="none" output="Y"/>
            <when state="4" output="Ÿ"/>
        </action>
        <action id="a">
            <when state="none" output="a"/>
            <when state="1" output="á"/>
            <when state="2" output="à"/>
            <when state="3" output="â"/>
            <when state="4" output="ä"/>
            <when state="5" output="ã"/>
        </action>
        <action id="a0">
            <when state="none" next="1"/>
        </action>
        <action id="a1">
            <when state="none" next="2"/>
        </action>
        <action id="a2">
            <when state="none" next="3"/>
        </action>
        <action id="a3">
            <when state="none" next="4"/>
        </action>
        <action id="a4">
            <when state="none" next="5"/>
        </action>
        <!-- These next actions correspond to what
        is output when letters who can be modified by
        dead keys (aeiouynAEIOUYN) are pressed,
        based on the current state of the system. -->
        <action id="e">
            <when state="none" output="e"/>
            <when state="1" output="é"/>
            <when state="2" output="è"/>
            <when state="3" output="ê"/>
            <when state="4" output="ë"/>
        </action>
        <action id="i">
            <when state="none" output="i"/>
            <when state="1" output="í"/>
            <when state="2" output="ì"/>
            <when state="3" output="î"/>
            <when state="4" output="ï"/>
        </action>
        <action id="n">
            <when state="none" output="n"/>
            <when state="5" output="ñ"/>
        </action>
        <action id="o">
            <when state="none" output="o"/>
            <when state="1" output="ó"/>
            <when state="2" output="ò"/>
            <when state="3" output="ô"/>
            <when state="4" output="ö"/>
            <when state="5" output="õ"/>
        </action>
        <action id="spacebar">
            <when state="none" output=" "/>
            <when state="1" output="´"/>
            <when state="2" output="`"/>
            <when state="3" output="ˆ"/>
            <when state="4" output="¨"/>
            <when state="5" output="˜"/>
        </action>
        <action id="u">
            <when state="none" output="u"/>
            <when state="1" output="ú"/>
            <when state="2" output="ù"/>
            <when state="3" output="û"/>
            <when state="4" output="ü"/>
        </action>
        <action id="y">
            <when state="none" output="y"/>
            <when state="4" output="ÿ"/>
        </action>
    </actions>
    <terminators>
        <!-- This section also controls the
        dead keys. There is probably no
        reason to ever touch this. -->
        <when state="1" output="´"/>
        <when state="2" output="`"/>
        <when state="3" output="ˆ"/>
        <when state="4" output="¨"/>
        <when state="5" output="˜"/>
    </terminators>
</keyboard>
